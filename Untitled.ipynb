    "sr_lst = []\n",
    "for n in range(len(df)):\n",
    "    sr = race.racer_sr()\n",
    "    racer_l = df.values[n][:7]\n",
    "    racer = [re.sub(\"  |\\u3000\", \" \", r) for r in racer_l if type(r) == str]\n",
    "    # print(racer)\n",
    "    # -> ['城戸 徹 41歳/27期 V0/0回/V1 ヤブキ３７３/1', '飯 塚', \n",
    "    # -> '0m 3.89 0.073', 'B-43 (B-46) 53.433', '3.39 3.463 3.423', '着順：0-0-0-7 0.16 ..']\n",
    "    fstname, lstname, age, v, machine = racer[0].split()\n",
    "    lg = racer[1].replace(\" \", \"\")\n",
    "    handicap, tryLap, tryDev = racer[2].split() # ハンデ、試走タイム、試走偏差、予想タイム\n",
    "    rank, prvRank, point = racer[3].split() # ランク、(前ランク)、審査ポイント\n",
    "    avgTry, avgLap, fstLap = racer[4].split() # 良10走lap\n",
    "    # print(tryLap)\n",
    "    last10, ast = racer[5].split()[:2] # 近10走着順、平均ST\n",
    "    sr[\"no\"] = n + 1\n",
    "    sr[\"name\"] = fstname + \" \" + lstname\n",
    "    sr[\"age\"] = re.sub(\"/\", \"-\", age)\n",
    "    sr[\"rank\"] = rank\n",
    "    sr[\"prank\"] = prvRank\n",
    "    sr[\"v\"] = re.sub(\"/\", \"-\", v)\n",
    "    sr[\"point\"] = float(point)\n",
    "    sr[\"last\"] = last10.strip(\"着順：\")\n",
    "    sr['ast'] = ast\n",
    "    sr[\"hand\"] = handicap\n",
    "    sr[\"machine\"] = re.sub(\"/1\", \"\", machine)\n",
    "    sr[\"lg\"] = lg\n",
    "    sr[\"try\"] = 0.0\n",
    "    sr[\"prd\"] = 0.0\n",
    "    if race.is_num(tryLap):\n",
    "        sr[\"try\"] = float(tryLap)\n",
    "        sr[\"prd\"] = float(tryLap) + float(tryDev)\n",
    "    sr[\"avt\"] = float(avgTry)\n",
    "    sr[\"avg\"] = float(avgLap)\n",
    "    sr[\"fst\"] = float(fstLap)\n",
    "    sr.name = \"\"\n",
    "    sr_lst.append(sr)\n",
    "\n",
    "def calc_goalDifs(laps, hands, mps=0.034):\n",
    "    \n",
    "    f_hands = [float(hand.strip(\"m\")) for hand in hands]\n",
    "    if sum(laps) != 0.0:\n",
    "        laps[0] = laps[0] + 0.01\n",
    "    goalTimes = [lap * (31.0 + f_hand/100) for lap, f_hand in zip(laps, f_hands)]\n",
    "    topTime = min(goalTimes)\n",
    "    goalDiffs = [(goalTime - topTime) / mps for goalTime in goalTimes]\n",
    "    \n",
    "    return goalDiffs\n",
    "\n",
    "hands = [sr[\"hand\"] for sr in sr_lst]\n",
    "avgLaps = [sr[\"avg\"] for sr in sr_lst]\n",
    "fstLaps = [sr[\"fst\"] for sr in sr_lst]\n",
    "prdLaps = [sr[\"prd\"] for sr in sr_lst]\n",
    "avgDifs = calc_goalDifs(avgLaps, hands)\n",
    "fstDifs = calc_goalDifs(fstLaps, hands)\n",
    "prdDifs = calc_goalDifs(prdLaps, hands)\n",
    "for sr, avgDif, fstDif, prdDif in zip(sr_lst, avgDifs, fstDifs, prdDifs):\n",
    "    sr[\"avm\"] = avgDif\n",
    "    sr[\"fsm\"] = fstDif\n",
    "    sr[\"pdm\"] = prdDif\n",
    "\n",
    "pd.DataFrame(sr_lst).dropna(how=\"all\", axis=1)"
   ]
